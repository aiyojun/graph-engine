{
  "name": "graph-engine",
  "version": "1.0.0",
  "description": "Modern graph rendering engine based on node.",
  "author": "aiyojun <1608450902@qq.com>",
  "homepage": "https://github.com/aiyojun/graph/tree/main/packages/graph-engine#readme",
  "license": "ISC",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aiyojun/graph.git"
  },
  "scripts": {
    "build": "tsc -p . && npm run postproc0 && npm run postproc1 && npm run postproc2",
    "postproc0": "node scripts/cmdline.js cp src/*.css dist/",
    "postproc1": "node scripts/cmdline.js cp-r src/assets dist/assets/",
    "postproc2": "node scripts/cmdline.js cp-r src/theme dist/theme/"
  },
  "bugs": {
    "url": "https://github.com/aiyojun/graph/issues"
  },
  "devDependencies": {
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "uuid": "^9.0.0"
  }
}
